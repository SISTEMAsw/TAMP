<?xml version="1.0" encoding="UTF-8"?>
<wps:Execute version="1.0.0" service="WPS" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://www.opengis.net/wps/1.0.0" xmlns:wfs="http://www.opengis.net/wfs" xmlns:wps="http://www.opengis.net/wps/1.0.0" xmlns:ows="http://www.opengis.net/ows/1.1" xmlns:gml="http://www.opengis.net/gml" xmlns:ogc="http://www.opengis.net/ogc" xmlns:wcs="http://www.opengis.net/wcs/1.1.1" xmlns:xlink="http://www.w3.org/1999/xlink" xsi:schemaLocation="http://www.opengis.net/wps/1.0.0 http://schemas.opengis.net/wps/1.0.0/wpsAll.xsd">
  <ows:Identifier>retrieve_data</ows:Identifier>
  <wps:DataInputs>
    <wps:Input>
      <ows:Identifier>collection_ids</ows:Identifier>
      <wps:Data>
        <wps:LiteralData>{{collection_ids}}</wps:LiteralData>
      </wps:Data>
    </wps:Input>
    <wps:Input>
      <ows:Identifier>csv_time_format</ows:Identifier>
      <wps:Data>
        <wps:LiteralData>Unix epoch</wps:LiteralData>
      </wps:Data>
    </wps:Input>
    {{#if model_ids}}<wps:Input>
      <ows:Identifier>model_ids</ows:Identifier>
      <wps:Data>
        <wps:LiteralData>{{model_ids}}</wps:LiteralData>
      </wps:Data>
    </wps:Input>{{/if}}
    {{#if shc}}<wps:Input>
      <ows:Identifier>shc</ows:Identifier>
      <wps:Data>
        <wps:ComplexData>{{shc}}</wps:ComplexData>
      </wps:Data>
    </wps:Input>{{/if}}
    {{#if bbox}}<wps:Input>
      <ows:Identifier>bbox</ows:Identifier>
      <wps:Data>
        <wps:ComplexData>{{bbox}}</wps:ComplexData>
      </wps:Data>
    </wps:Input>{{/if}}
    <wps:Input>
      <ows:Identifier>begin_time</ows:Identifier>
      <wps:Data>
        <wps:LiteralData>{{begin_time}}</wps:LiteralData>
      </wps:Data>
    </wps:Input>
    <wps:Input>
      <ows:Identifier>end_time</ows:Identifier>
      <wps:Data>
        <wps:LiteralData>{{end_time}}</wps:LiteralData>
      </wps:Data>
    </wps:Input>
  </wps:DataInputs>
  <wps:ResponseForm>
    <wps:RawDataOutput mimeType="text/csv">
      <ows:Identifier>output</ows:Identifier>
    </wps:RawDataOutput>
  </wps:ResponseForm>
</wps:Execute>
